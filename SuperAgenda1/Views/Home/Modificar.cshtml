@model SuperAgenda1.Models.Contactos
@{ }

<div class="container">
    <form method="Post" action="/Home/Modificar" class="Form-horizontal">
        @Html.HiddenFor(obj => obj.Id)
        <div class="form-group">
            @Html.LabelFor(a => a.Nombres, new { @class = "col-2" })
            <div class="col-lg-4">
                @Html.TextBoxFor(a => a.Nombres, new { @class = "form-control" })
            </div>
            @Html.LabelFor(a => a.Apellidos, new { @class = "col-2" })
            <div class="col-lg-4">
                @Html.TextBoxFor(a => a.Apellidos, new { @class = "form-control" })
            </div>
            @Html.LabelFor(a => a.TelefonoCasa, new { @class = "col-2" })
            <div class="col-lg-4">
                @Html.TextBoxFor(a => a.TelefonoCasa, new { @class = "form-control" })
            </div>
            @Html.LabelFor(a => a.TelefonoMovil, new { @class = "col-2" })
            <div class="col-lg-4">
                @Html.TextBoxFor(a => a.TelefonoMovil, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-2">Tipo De Contacto</label>
            <div class="col-4">
                @Html.DropDownListFor(a => a.TipoContacto, Html.GetEnumSelectList<SuperAgenda1.Models.TipoContacto>(), new { @class = "form-control" })
            </div>
        </div>
        <button type="submit" class="btn btn-success"> Guardar contacto </button>

    </form>
</div>